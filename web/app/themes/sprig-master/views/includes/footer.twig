<footer class="content-info" role="contentinfo">
  <div class="container">
    <div class="left">
        <div class="copyright">{{default.footer_gauche}}</div>
        <div class="contact">
            <a href="mailto:{{default.email}}">{{default.email}}</a>
        </div>
    </div>
    <div class="right">
        {% if default.facebook_liens|length != 0 %}
        <a target="_blank" href="{{default.facebook_liens}}" class="icon-facebook"></a>
        {% endif %}

        {% if default.instagram_liens|length != 0 %}
        <a target="_blank" href="{{default.instagram_liens}}" class="icon-instagram"></a>
        {% endif %}

        {% if default.twitter_liens|length != 0 %}                        
        <a target="_blank" href="{{default.twitter_liens}}" class="icon-twitter"></a>
        {% endif %}
    </div>
  </div>
</footer>



