<header class="banner navbar navbar-static-top" role="banner">
  <div class="container">
    <div class="head-contacts">
        <div class="contacts">
            <div class="address">{{default.adresse}}, {{default.cp_ville}}</div>
            <a href="mailto:{{default.email}}">{{default.email}}</a>
            <div class="tel">{{default.tel}}</div>
        </div>

        <div class="social"></div>
    </div>
    <div class="logo"><a href="{{function('get_permalink', 4)}}"><img src="{{theme.link}}/img/logo.jpg" alt="logo" /></a></div>
    <nav class="main-nav content" role="navigation">
            <div>

                <ul>
                {% for item in menu.items %}
                    <li id="menu-item-{{item.ID}}" class="menu-item menu-item-type-{{item.type}} menu-item-object-{{item.object}} menu-item-{{item.ID}} {{item.slug}} {% if post.slug == item.slug %} current {% endif %}"><a href="{{item.link}}">{{item.title}}</a></li>
                {% endfor %}
                    {% if function('getObjectListExist') %}
                    <li id="menu-item-{{menu.items|length}}" class="menu-item menu-item-{{menu.items|length}} {% if post.slug == 'wishlist' %} current {% endif %}"><a href="{{function('get_permalink', 127)}}"><span class="icon-basket"></span> Ma Liste</a></li>
                    {% endif %}
                </ul>
                
            </div>
    </nav>
  </div>
</header>
